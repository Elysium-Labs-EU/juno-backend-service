{
  "name": "juno_backend",
  "version": "1.0.0",
  "description": "Backend server for Juno",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "tsc && node dist/server.js",
    "start:watch": "nodemon",
    "lint": "eslint \"src/**/*.{js,jsx,json,md,tsx,ts}\"",
    "lint-fix": "eslint \"src/**/*.{js,jsx,json,md,tsx,ts}\" --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,md,tsx,ts}\"",
    "lint:prettier": "prettier --check \"src/**/*.{js,jsx,json,md,tsx,ts}\""
  },
  "author": "@sgnilreutR",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/local-auth": "^1.0.1",
    "express": "^4.17.2",
    "gaxios": "^4.3.2",
    "google-auth-library": "^7.11.0",
    "googleapis": "^92.0.0",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0",
    "ts-loader": "^9.2.6",
    "typesync": "^0.8.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.13",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn start",
    "ext": "ts"
  }
}
