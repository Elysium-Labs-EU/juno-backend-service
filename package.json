{
  "name": "juno_backend",
  "version": "1.0.1",
  "description": "Backend server for Juno",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "tsc && node dist/server.js",
    "start:esbuild": "node esbuild.config.js && node out.js",
    "start:watch": "nodemon",
    "lint": "eslint \"src/**/*.{js,jsx,json,md,tsx,ts}\"",
    "lint-fix": "eslint \"src/**/*.{js,jsx,json,md,tsx,ts}\" --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,md,tsx,ts}\"",
    "lint:prettier": "prettier --check \"src/**/*.{js,jsx,json,md,tsx,ts}\"",
    "lint-staged": "lint-staged --config lint-staged.config.mjs",
    "husky-install": "husky install"
  },
  "author": "@sgnilreutR",
  "license": "ISC",
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^1.0.1",
    "@sentry/node": "^6.19.2",
    "@sentry/tracing": "^6.19.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "esbuild": "^0.14.27",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "gaxios": "^4.3.2",
    "googleapis": "^100.0.0",
    "supertokens-node": "^9.1.2",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0",
    "ts-loader": "^9.2.8",
    "typesync": "^0.9.0"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/node": "17.0.23",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/swagger-ui-express": "4.1.3",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "eslint": "8.12.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.25.4",
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "nodemon": "2.0.15",
    "prettier": "2.6.1",
    "ts-node": "10.7.0",
    "tsc-files": "1.1.3",
    "typescript": "4.6.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn start:esbuild",
    "ext": "ts"
  }
}
