{
  "name": "juno_backend",
  "version": "0.1.9",
  "type": "module",
  "description": "Backend server for Juno",
  "main": "server.js",
  "scripts": {
    "start": "node esbuild.config.js && node --inspect out.js",
    "start:watch": "nodemon",
    "start:tsc": "tsc && node dist/server.js",
    "start:watch:dev": "cp ./src/environments/.env.dev .env && nodemon",
    "start:watch:dev:win": "copy .env .env.dev  && nodemon",
    "lint": "eslint \"src/**/*.{js,json,md,ts}\"",
    "lint-fix": "eslint \"src/**/*.{js,json,md,ts}\" --fix",
    "format-check": "dprint check",
    "format": "dprint fmt",
    "lint-staged": "lint-staged --config lint-staged.config.mjs",
    "husky-install": "husky install",
    "test": "vitest",
    "typecheck": "tsc --incremental --noEmit",
    "update-deps": "yarn upgrade-interactive --latest"
  },
  "author": {
    "name": "Robbert Tuerlings",
    "email": "robbert@elysiumlabs.io",
    "url": "https://elysiumlabs.io"
  },
  "license": "ISC",
  "dependencies": {
    "@logtail/node": "^0.4.5",
    "@sentry/node": "^7.60.0",
    "@sentry/profiling-node": "^1.1.2",
    "@sentry/tracing": "^7.60.0",
    "autolinker": "^4.0.0",
    "base64-arraybuffer": "^1.0.2",
    "base64url": "^3.0.1",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "dotenv": "^16.3.1",
    "esbuild": "^0.18.16",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "formidable": "3.5.0",
    "gaxios": "^6.0.2",
    "googleapis": "^122.0.0",
    "helmet": "^7.0.0",
    "redis": "^4.6.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.10.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/compression": "1.7.2",
    "@types/connect-redis": "0.0.20",
    "@types/eslint": "8.21.3",
    "@types/express": "4.17.17",
    "@types/express-session": "1.17.6",
    "@types/formidable": "3.4.0",
    "@types/node": "18.15.3",
    "@types/nodemon": "1.19.2",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/swagger-ui-express": "4.1.3",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "dprint": "0.39.1",
    "eslint": "8.36.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-import": "2.27.5",
    "husky": "8.0.3",
    "lint-staged": "13.2.0",
    "nodemon": "3.0.1",
    "ts-node": "10.9.1",
    "ts-to-zod": "^3.0.0",
    "tsc-files": "1.1.4",
    "typescript": "5.1.6",
    "vitest": "^0.33.0"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn start",
    "ext": "ts"
  }
}
