{
  "name": "juno_backend",
  "version": "1.0.1",
  "type": "module",
  "description": "Backend server for Juno",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node esbuild.config.js && node --inspect out.js",
    "start:watch": "nodemon",
    "start:tsc": "tsc && node dist/server.js",
    "start:watch:dev": "cp ./src/environments/.env.dev  .env && nodemon",
    "start:watch:dev:win": "copy .env .env.dev  && nodemon",
    "lint": "eslint \"src/**/*.{js,jsx,json,md,tsx,ts}\"",
    "lint-fix": "eslint \"src/**/*.{js,jsx,json,md,tsx,ts}\" --fix",
    "format-check": "dprint check",
    "format": "dprint fmt",
    "lint-staged": "lint-staged --config lint-staged.config.mjs",
    "husky-install": "husky install",
    "update-deps": "yarn upgrade-interactive --latest"
  },
  "author": {
    "name": "Robbert Tuerlings",
    "email": "robbert@elysiumlabs.io",
    "url": "https://elysiumlabs.io"
  },
  "license": "ISC",
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@sentry/node": "^7.23.0",
    "@sentry/tracing": "^7.23.0",
    "autolinker": "^4.0.0",
    "base64-arraybuffer": "^1.0.2",
    "base64url": "^3.0.1",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "connect-redis": "^6.1.3",
    "dotenv": "^16.0.3",
    "esbuild": "^0.15.16",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "formidable": "3.2.4",
    "gaxios": "^5.0.2",
    "googleapis": "^109.0.1",
    "helmet": "^6.0.1",
    "redis": "^4.5.1",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.6.0",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@types/compression": "1.7.2",
    "@types/connect-redis": "0.0.18",
    "@types/eslint": "8.4.10",
    "@types/express": "4.17.14",
    "@types/express-session": "1.17.5",
    "@types/formidable": "2.0.5",
    "@types/node": "18.11.9",
    "@types/nodemon": "1.19.2",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/swagger-ui-express": "4.1.3",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "dprint": "0.33.0",
    "eslint": "8.29.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "husky": "8.0.2",
    "lint-staged": "13.0.4",
    "nodemon": "2.0.20",
    "ts-node": "10.9.1",
    "tsc-files": "1.1.3",
    "typescript": "4.9.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn start",
    "ext": "ts"
  }
}
