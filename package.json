{
  "name": "juno_backend",
  "version": "1.0.1",
  "description": "Backend server for Juno",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node esbuild.config.js && node out.js",
    "start:watch": "nodemon",
    "start:tsc": "tsc && node dist/server.js",
    "start:watch:dev": "cp ./src/environments/.env.dev  .env && nodemon",
    "start:watch:dev:win": "copy .env .env.dev  && nodemon",
    "lint": "eslint \"src/**/*.{js,jsx,json,md,tsx,ts}\"",
    "lint-fix": "eslint \"src/**/*.{js,jsx,json,md,tsx,ts}\" --fix",
    "format": "dprint check",
    "lint-staged": "lint-staged --config lint-staged.config.mjs",
    "husky-install": "husky install",
    "update-deps": "yarn upgrade-interactive --latest"
  },
  "author": "@sgnilreutR",
  "license": "ISC",
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^2.0.0",
    "@sentry/node": "^7.7.0",
    "@sentry/tracing": "^7.7.0",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.1",
    "esbuild": "^0.14.49",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "gaxios": "^5.0.1",
    "googleapis": "^105.0.0",
    "redis": "^4.2.0",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.5.0",
    "ts-loader": "^9.3.1",
    "typesync": "^0.9.2"
  },
  "devDependencies": {
    "@types/connect-redis": "0.0.18",
    "@types/express": "4.17.13",
    "@types/express-session": "1.17.5",
    "@types/node": "18.0.5",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/swagger-ui-express": "4.1.3",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "dprint": "0.30.3",
    "eslint": "8.20.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "nodemon": "2.0.19",
    "ts-node": "10.9.1",
    "tsc-files": "1.1.3",
    "typescript": "4.7.4"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn start",
    "ext": "ts"
  }
}
