{
  "name": "juno_backend",
  "version": "1.0.0",
  "description": "Backend server for Juno",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node esbuild.config.js && node out.js",
    "start:watch": "nodemon",
    "lint": "eslint \"src/**/*.{js,jsx,json,md,tsx,ts}\"",
    "lint-fix": "eslint \"src/**/*.{js,jsx,json,md,tsx,ts}\" --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,md,tsx,ts}\"",
    "lint:prettier": "prettier --check \"src/**/*.{js,jsx,json,md,tsx,ts}\""
  },
  "author": "@sgnilreutR",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/local-auth": "^1.0.1",
    "cookie-parser": "^1.4.6",
    "esbuild": "^0.14.21",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "gaxios": "^4.3.2",
    "google-auth-library": "^7.12.0",
    "googleapis": "^95.0.0",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0",
    "ts-loader": "^9.2.6",
    "typesync": "^0.8.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.18",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn start",
    "ext": "ts"
  }
}
